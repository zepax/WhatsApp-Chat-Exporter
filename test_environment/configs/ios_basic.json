{
    "name": "iOS Basic Test",
    "description": "Configuración básica para pruebas con backup de iTunes/Finder",
    "platform": "ios",
    "encrypted": false,
    "files": {
        "backup_dir": "ios/backup",
        "required_files": [
            "Manifest.db",
            "Info.plist"
        ]
    },
    "options": {
        "output_formats": ["html", "json"],
        "include_media": true,
        "generate_summary": true,
        "extract_media": true,
        "verbose": true
    },
    "expected_outputs": [
        "result.json",
        "summary.json",
        "*.html"
    ],
    "dependencies": [
        "iphone_backup_decrypt"
    ]
}